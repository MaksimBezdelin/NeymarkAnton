from telegram import Update, ReplyKeyboardMarkup, InlineKeyboardButton, InlineKeyboardMarkup
from telegram.ext import ApplicationBuilder, CommandHandler, ContextTypes, MessageHandler, filters
import sqlite3

# –í–∞—à —Ç–æ–∫–µ–Ω –±–æ—Ç–∞
TOKEN = '7650228237:AAFEeIjY8YSEjlhNExxGgiS8t2nz4j7kgEc'
# URL –≤–∞—à–µ–≥–æ WebApp
WEB_APP_URL = 'https://coin-dusky-psi.vercel.app/'

conn = sqlite3.connect('neymark.db')
cursor = conn.cursor()

async def send_welcome_message(update: Update, username: str) -> None:
    message = (f'–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π—Ç–µ, {username}, —á–µ–º –º–æ–≥—É –±—ã—Ç—å –ø–æ–ª–µ–∑–µ–Ω? ')
    await update.message.reply_text(message, parse_mode='HTML')

async def start(update: Update, context: ContextTypes.DEFAULT_TYPE) -> None:
    username = update.effective_user.first_name
    await send_welcome_message(update, username)
    keyboard = [
        ['–ü–æ–ª—É—á–∏—Ç—å –æ–ø—Ä–æ—Å üìù'],
        ['–ü—Ä–æ—Ñ–∏–ª—å üë§'],
        ['–û–±–º–µ–Ω—è—Ç—å –±–∞–ª–ª—ã üéÅ'],
        ["–†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è"],
        [InlineKeyboardButton("Lets go ‚òÄÔ∏è", web_app={"url": WEB_APP_URL})]
    ]
    reply_markup = ReplyKeyboardMarkup(keyboard[:-1], resize_keyboard=True)  # –û–±—ã—á–Ω—ã–µ –∫–Ω–æ–ø–∫–∏
    await update.message.reply_text('–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:', reply_markup=reply_markup)
    inline_keyboard = [[InlineKeyboardButton("Lets go ‚òÄÔ∏è", web_app={"url": WEB_APP_URL})]]
    inline_reply_markup = InlineKeyboardMarkup(inline_keyboard)
    await update.message.reply_text('–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:', reply_markup=inline_reply_markup)
async def button_handler(update: Update,  context: ContextTypes.DEFAULT_TYPE) -> None:
    
    user_message = update.message.text
    usernames = update.effective_user.username
    reg = 'SELECT * FROM neymark WHERE username1 = ?'
    cursor.execute(reg, (usernames,))
    data = cursor.fetchone()
    age = 1
    if user_message == '–ü–æ–ª—É—á–∏—Ç—å –æ–ø—Ä–æ—Å üìù':
        context.user_data.pop('registering', None)
        if data:
            print(data)
            keyboard = [
                ['–§–∏–∑–º–∞—Ç üë®‚Äçüíª'],
                ['–ì—É–º–∞–Ω–∏—Ç–∞—Ä–∏–π üë®‚Äçüéì'],
                ['–•–∏–º–±–∏–æ üë®‚Äçüî¨'],
                ['–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é üìã'],
                [InlineKeyboardButton("Lets go ‚òÄÔ∏è", web_app={"url": WEB_APP_URL})]
            ]
            reply_markup = ReplyKeyboardMarkup(keyboard[:-1], resize_keyboard=True)  # –û–±—ã—á–Ω—ã–µ –∫–Ω–æ–ø–∫–∏
            await update.message.reply_text("–ò—Ç–∞–∫, –Ω–∞—á–Ω—ë–º! –ö–µ–º —Ç—ã —Å–µ–±—è —Å—á–∏—Ç–∞–µ—à—å?", reply_markup=reply_markup)
            return

        
    
    if user_message == '–†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è':
        print("1")
        context.user_data.pop('registering', None)
        if data:
            await update.message.reply_text("–í—ã —É–∂–µ –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞–Ω—ã")
        context.user_data['registering'] = ''
        if 'registering' in context.user_data:
            if context.user_data['registering'] == '':
                await update.message.reply_text('–í–∏–∂—É —á—Ç–æ –≤—ã –Ω–µ –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞–Ω—ã. –î–∞–≤–∞–π—Ç–µ –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä—É–µ–º—Å—è.')
            

        if context.user_data['registering'] == 'FIO':
            context.user_data['FIO'] = user_message
        elif 'FIO' not in context.user_data:
            context.user_data['registering'] = 'FIO'
            await update.message.reply_text('–í–≤–µ–¥–∏—Ç–µ –≤–∞—à–µ –§–ò–û:')
            print('2')
        if context.user_data['registering'] == 'city':
            context.user_data['city'] = user_message
        elif 'city' not in context.user_data:
            await update.message.reply_text('–í –∫–∞–∫–æ–º –≥–æ—Ä–æ–¥–µ –≤—ã –∂–∏–≤—ë—Ç–µ:')
            print("3")
            return

        ball = 0
        cursor.execute('INSERT INTO neymark (user_id, FIO, city, username1, ball) VALUES (?, ?,?,?, ?)',
                   (context._user_id, context.user_data['FIO'], context.user_data['city'], usernames, ball))
        conn.commit()
        await update.message.reply_text('–†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è –ø—Ä–æ—à–ª–∞ —É—Å–ø–µ—à–Ω–æ!')
        # –£–±–∏—Ä–∞–µ–º —Ñ–ª–∞–≥ –∏ –¥–∞–Ω–Ω—ã–µ –ø–æ—Å–ª–µ –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è —Ä–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏–∏
        context.user_data.pop('registering', None)
        context.user_data.pop('FIO', None)
        context.user_data.pop('city', None)
        
        


    
    if user_message == '–§–∏–∑–º–∞—Ç üë®‚Äçüíª':
        await update.message.reply_text("–ó–Ω–∞—á–∏—Ç, —ç—Ç—É –∏—Å—Ç–æ—Ä–∏—é —Ç–µ–±–µ —Ä–∞—Å—Å–∫–∞–∂–µ—Ç –ú–∞—à–∞! –û–∑–Ω–∞–∫–æ–º—å—Å—è —Å –µ—ë –∞–Ω–∫–µ—Ç–æ–π")
        await update.message.reply_photo(photo=open("GIRLS\–ú–∞—Ç–µ—à–∞.jpg", "rb"),
                                         caption="–ü—Ä–∏–≤–µ—Ç! –ú–µ–Ω—è –∑–æ–≤—É—Ç –ú–∞—à–∞, —è –æ—á–µ–Ω—å –ª—é–±–ª—é –º–∞—Ç–µ–º–∞—Ç–∏–∫—É –∏ –º–µ—á—Ç–∞—é –ø–æ—Å—Ç—É–ø–∏—Ç—å –≤ –ú–ì–¢–£ –∏–º –ë–∞—É–º–∞–Ω–∞. –õ—é–±–ª—é –ø–æ–∏–≥—Ä–∞—Ç—å –≤ –∫–∞—Ä—Ç—ã, —Ç–æ–ª—å–∫–æ —Ç—Å—Å... –ù–∏–∫–æ–º—É! –í–∑—Ä–æ—Å–ª—ã–µ —ç—Ç–æ–≥–æ –Ω–µ –æ–¥–æ–±—Ä—è—é—Ç, –∞ —è —Å—á–∏—Ç–∞—é —ç—Ç–æ —Ç—Ä–µ–Ω–∏—Ä–æ–≤–∫–æ–π –ø—Ä–∏–º–µ–Ω–µ–Ω–∏—è —Ç–µ–æ—Ä–∏–∏ –≤–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç–µ–π ;) –¶–≤–µ—Ç—ã –º–Ω–µ –¥–∞–∂–µ –Ω–µ —Å—É–π ‚Äî –Ω–µ—Ä–∞—Ü–∏–æ–Ω–∞–ª—å–Ω–∞—è —Ç—Ä–∞—Ç–∞ –¥–µ–Ω–µ–≥")
        keyboard = [
            ['–ù–∞—á–∞—Ç—å üë®‚Äçüíª'],
            ['–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é üìã'],
            [InlineKeyboardButton("Lets go ‚òÄÔ∏è", web_app={"url": WEB_APP_URL})]
        ]
        reply_markup = ReplyKeyboardMarkup(keyboard[:-1], resize_keyboard=True)  # –û–±—ã—á–Ω—ã–µ –∫–Ω–æ–ø–∫–∏
        await update.message.reply_text("–î–∞–≤–∞–π –Ω–∞—á–Ω—ë–º?", reply_markup=reply_markup)
        return
    if user_message == '–ù–∞—á–∞—Ç—å üë®‚Äçüíª':
        await update.message.reply_photo(photo=open("GIRLS\–ú–∞—Ç–µ—à–∞1.jpg", "rb"),
                                         caption="–ú–∞—à–∞ –∑–æ–≤—ë—Ç –Ω–∞—Å –≥—É–ª—è—Ç—å, –∏ –º—ã —Å–æ–≥–ª–∞—à–∞–µ–º—Å—è.")
        await update.message.reply_text("–í—ã –∏–¥—ë—Ç–µ –ø–æ —É–ª–∏—Ü–µ, –º–∏–ª–æ –±–µ—Å–µ–¥—É—è, –∫–∞–∫ –≤–¥—Ä—É–≥ –æ–Ω–∞ –≥–æ–≤–æ—Ä–∏—Ç:"),
                                         
