    if user_message == '12 üë®‚Äçüíª' or user_message == '13 üë®‚Äçüíª' or user_message == '14 üë®‚Äçüíª' or user_message == '15 üë®‚Äçüíª' or user_message == '16 üë®‚Äçüíª' or user_message == "17 üë®‚Äçüíª" or user_message == "18 üë®‚Äçüíª":
        age = user_message
        print(age)

        await update.message.reply_photo(photo=open("GIRLS\–ú–∞—Ç–µ—à–∞3.jpg", "rb"),
                                         caption="–ó–¥–æ—Ä–æ–≤–æ! –ú–Ω–µ —Ç–æ–∂–µ)")
        # –ì–¥–µ-–Ω–∏–±—É–¥—å —Ç—É—Ç –≤–æ—Ç–∫–Ω–∏ –≤–æ–ø—Ä–æ—Å –ø—Ä–æ —É–≤–ª–µ—á–µ–Ω–∏—è
        await update.message.reply_text("–í—ã –∏–¥—ë—Ç–µ –≥—É–ª—è—Ç—å –¥–∞–ª—å—à–µ, –∏ –≤—ã—Ö–æ–¥–∏—Ç–µ –Ω–∞ –ø–ª–æ—â–∞–¥—å, –≥–¥–µ –µ—Å—Ç—å –º–Ω–æ–≥–æ —Ä–∞–∑–Ω—ã—Ö –º–∞–≥–∞–∑–∏–Ω–æ–≤. –¢—ã —Ö–æ—á–µ—à—å —Å–¥–µ–ª–∞—Ç—å –Ω–µ–±–æ–ª—å—à–æ–π –ø–æ–¥–∞—Ä–æ–∫ —Å–≤–æ–µ–π –æ—á–∞—Ä–æ–≤–∞—Ç–µ–ª—å–Ω–æ–π —Å–ø—É—Ç–Ω–∏—Ü–µ")
        
