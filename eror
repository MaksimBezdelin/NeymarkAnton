        keyboard = [
            ['12 üë®‚Äçüíª'],
            ['13 üë®‚Äçüíª'],
            ['14 üë®‚Äçüíª'],
            ['15 üë®‚Äçüíª'],
            ['16 üë®‚Äçüíª'],
            ['17 üë®‚Äçüíª'],
            ['18 üë®‚Äçüíª'],
